create table if not exists users (id bigint primary key, name varchar(20));
create table if not exists PaymentTables (id bigint primary key, tableName varchar(1024), cursor bigint, updateTime bigint);
CREATE TABLE if not exists ActionRecord (id bigint primary key,transactionID varchar(32) ,errorCode integer,time timestamp,message varchar(255));
CREATE TABLE if not exists CheckbookItem (id bigint primary key,transactionID varchar(32) ,status tinyint,business varchar(64) ,referenceID varchar(100) ,type varchar(16), externalTransactionID varchar(64),requestedAmount varchar(32),acknowledgedAmount varchar(32),recordTime timestamp,transactionTime timestamp,paymentMethod varchar(16) ,uniqueKey varchar(128));
CREATE TABLE if not exists CheckOrderDiffItem (id bigint primary key,transactionID varchar(32) ,status tinyint,createTime timestamp,lastUpdateTime timestamp,referenceID varchar(100),paymentMethod varchar(16) ,operationType varchar(16),acknowledgedAmount varchar(32),business varchar(255) ,customerID bigint,jobId varchar(64), externalTransactionID varchar(64));
CREATE TABLE if not exists FastSearchTableItem (id bigint primary key,itemKey varchar(255) ,transactionID varchar(32),time timestamp,status integer,message varchar(1046));
CREATE TABLE if not exists PaymentRequest (id bigint primary key,referenceIDList varchar(100),status integer,channel varchar(10) ,createTime timestamp,lastUpdateTime timestamp,requestedAmount varchar(20) ,paymentMethod varchar(10),customerID bigint ,paymentOperationType varchar(20) ,business varchar(20),callBackMetaInfo varchar(2048));
CREATE TABLE if not exists PaymentResponse (id bigint primary key,referenceID varchar(100),transactionID varchar(32),externalTransactionID varchar(64),status integer,createTime timestamp,lastUpdateTime timestamp,paymentMethod varchar(10),operationType varchar(20),acknowledgedamount varchar(20),business varchar(20),customerID bigint,rawResponse varchar(2048),db_update_time timestamp);
CREATE TABLE if not exists ScheduleJobRecord (id bigint primary key,jobID varchar(64),createTime timestamp,updateTime timestamp,jobstatus varchar(16) ,nextWorkTime timestamp,retryTimes smallint,description varchar(255) ,ext1 varchar(255) ,ext2 varchar(1204));
CREATE TABLE if not exists PaymentStatistic (id bigint primary key,business varchar(20),paymentMethod varchar(10),paymentOperation varchar(20),date integer,amount varchar(32),target varchar(100));
CREATE TABLE if not exists PaymentAccount (id bigint primary key, accountName varchar(255), accountNo varchar(255), paymentMethod varchar(30));
create table if not exists PaymentAccountScope (id bigint primary key, accountID bigint, supportedOperations varchar(255), issuingBanks varchar(255), issuer varchar(30), deviceType varchar(32), supportedBusinesses varchar(255));
